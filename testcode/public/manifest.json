{
  "name": "My PDF Exporter",
  "description": "Export the current page as a PDF",
  "version": "1.0",
  "manifest_version": 2,

  "permissions": ["activeTab", "tabs", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Export PDF"
  }
}
